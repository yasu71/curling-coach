{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
      "standard",
      "plugin:react/recommended",
      "prettier"
      ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-console": "warn",
      "react/prop-types": 0
    }
  }
  